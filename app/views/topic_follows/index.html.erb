<h1>Listing topic_follows</h1>

<table>
  <tr>
    <th>User</th>
    <th>Topic</th>
    <th>Manual follow</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @topic_follows.each do |topic_follow| %>
  <tr>
    <td><%= topic_follow.user_id %></td>
    <td><%= topic_follow.topic_id %></td>
    <td><%= topic_follow.manual_follow %></td>
    <td><%= link_to 'Show', topic_follow %></td>
    <td><%= link_to 'Edit', edit_topic_follow_path(topic_follow) %></td>
    <td><%= link_to 'Destroy', topic_follow, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

