<%= link_to content_tag(:span, nil, :class => 'ss-like') + "#{post.upvote_count_text}", vote_path(post, current_user, "up"), :class => @upvote_button_class, :remote => true %>

<%= link_to content_tag(:span, nil, :class => 'ss-dislike') + "#{post.downvote_count_text}", vote_path(post, current_user, "down"), :class => @downvote_button_class, :remote => true %>