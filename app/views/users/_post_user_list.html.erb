          <ul class='saved-articles list backend-user-container'>
            <% @posts.each do |post| %>
              <%= @case == 'drafts' ? (render 'drafts_written_by_user', :post => post, :topics => post.topics, :user => post.user) : (render 'post_written_by_user', :post => post, :topics => post.topics, :user => post.user) %>
            <% end %>
          </ul>
            